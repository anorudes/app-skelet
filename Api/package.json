{
  "name": "Api",
  "version": "1.0.0",
  "description": "Api",
  "scripts": {
    "lint": "eslint src && eslint bin",
    "db:sync:development": "NODE_ENV=development && babel-node ./scripts/db-sync.js",
    "db:sync:test": "NODE_ENV=test babel-node ./scripts/db-sync.js",
    "db:sync:production": "NODE_ENV=production && babel-node ./scripts/db-sync.js",
    "db:migrate:development": "sequelize db:migrate --env development",
    "db:migrate:test": "sequelize db:migrate --env test",
    "db:migrate:production": "sequelize db:migrate --env production",
    "db:scripts:development": "NODE_ENV=development && babel-node ./scripts/server-scripts.js",
    "db:scripts:test": "NODE_ENV=test && babel-node ./scripts/server-scripts.js",
    "db:scripts:production": "NODE_ENV=production && babel-node ./scripts/server-scripts.js",
    "db:update:development": "npm run db:sync:development && npm run db:migrate:development && npm run db:scripts:development",
    "db:update:test": "npm run db:sync:test && npm run db:migrate:test && npm run db:scripts:test",
    "db:update:production": "npm run db:sync:production && npm run db:migrate:production && npm run db:scripts:production",
    "start": "NODE_ENV=development NODE_DEBUG=true nodemon --watch src bin/start.js",
    "start:env": "node bin/start.js",
    "start:test": "NODE_ENV=test forever start bin/start.js",
    "start:prod": "NODE_ENV=production node bin/start.js",
    "start:debug": "NODE_ENV=development NODE_DEBUG=true concurrently --kill-others \"nodemon --debug --watch src bin/start.js\"  \"node-inspector -p 3020\"",
    "flow": "flow",
    "test": "jest"
  },
  "repository": {
    "type": "git"
  },
  "homepage": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-require-ignore": "0.1.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "flow-bin": "^0.58.0",
    "babel-preset-flow": "^6.23.0",
    "bcryptjs": "2.4.3",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "brototype": "0.0.6",
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "cookies-js": "1.2.3",
    "cors": "2.8.4",
    "es6-promise": "4.1.1",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-react": "7.4.0",
    "except": "0.1.3",
    "exports-loader": "0.6.4",
    "express": "4.16.2",
    "express-async-wrap": "1.0.0",
    "express-session": "1.15.6",
    "helmet": "3.9.0",
    "history": "4.7.2",
    "http-proxy": "1.16.2",
    "jwt-simple": "0.5.1",
    "lodash.throttle": "4.1.1",
    "markdown": "0.5.0",
    "md5": "2.2.1",
    "merge": "1.2.0",
    "mocha": "4.0.1",
    "morgan": "1.9.0",
    "multer": "1.3.0",
    "pg": "7.3.0",
    "ramda": "0.25.0",
    "request-ip": "2.0.2",
    "request-promise": "4.2.2",
    "sequelize": "4.22.1",
    "sinon": "4.1.1",
    "sinon-chai": "2.14.0",
    "socket.io": "2.0.4",
    "socket.io-redis": "^5.2.0",
    "stream-to-promise": "2.2.0",
    "superagent": "3.8.0",
    "superagent-bluebird-promise": "4.2.0",
    "superagent-jsonp": "0.1.1",
    "throttle-function": "0.1.0",
    "winston": "2.4.0",
    "winston-logstash": "0.3.0"
  },
  "main": "karma.conf.js",
  "bin": {
    "NewProject": "api.js"
  },
  "dependencies": {
    "redis": "^2.8.0",
    "sequelize-cli": "3.0.0"
  }
}
