{
  "name": "App",
  "version": "1.0.0",
  "description": "Mulbabar-App",
  "scripts": {
    "lint": "eslint src && eslint bin6 && eslint webpack",
    "start": "rimraf dist && NODE_ENV=development node bin/server.js",
    "start-ssr": "rimraf dist && NODE_ENV=development concurrently --kill-others \"nodemon --inspect --watch src bin/server-ssr.js\" \"node bin/webpack-server.js\"",
    "start-test": "NODE_ENV=test node bin/server-ssr.js",
    "start-test:watch": "NODE_ENV=test nodemon --inspect --watch src bin/server-ssr.js",
    "production-in-env": "node bin/server-ssr.js",
    "production": "NODE_ENV=production node bin/server-ssr.js",
    "production:watch": "NODE_ENV=production nodemon --inspect --watch src bin/server-ssr.js",
    "clean": "rimraf dist",
    "build": "rimraf dist && NODE_ENV=production webpack --progress --display-error-details --config webpack/common.config.js",
    "build:test": "rimraf dist && NODE_ENV=test webpack --progress --display-error-details --config webpack/common.config.js",
    "test:watch": "npm run test -- --watch",
    "story": "NODE_ENV=development start-storybook -s ./ -p 6006",
    "flow": "flow",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/genipigs/mulbabar-app"
  },
  "homepage": "http://aoh/",
